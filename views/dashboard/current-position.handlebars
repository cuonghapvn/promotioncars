</section>

<!-- Main content -->
<section class="content">

  <div class="row">
    <div class="col-lg-4 col-xs-6" id="map-hcm-btn">
      <!-- small box -->
      <div class="small-box bg-green">
        <div class="inner">
          <h3>Ho Chi Minh</h3>
          <p>75 xe</p>
        </div>
        <div class="icon">
          <i class="ion ion-model-s"></i>
        </div>
      </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-4 col-xs-6" id="map-dn-btn">
      <!-- small box -->
      <div class="small-box bg-aqua">
        <div class="inner">
          <h3>Da Nang</h3>
          <p>30 xe</p>
        </div>
        <div class="icon">
          <i class="ion ion-model-s"></i>
        </div>
      </div>
    </div>
    <!-- ./col -->
    <div class="col-lg-4 col-xs-6" id="map-hn-btn">
      <!-- small box -->
      <div class="small-box bg-red">
        <div class="inner">
          <h3>Ha Noi</h3>
          <p>50 xe</p>
        </div>
        <div class="icon">
          <i class="ion ion-model-s"></i>
        </div>
      </div>
    </div>
  </div>
  {{!-- maps --}}
  <div class="row">
    <div class="col-md-12">
      <div class="box">
        <!-- /.box-header -->
        <div class="box-body">
          <div class="row">
            <div class="col-md-12">
              <div id="current-position-map">
              </div>
              <!-- /.chart-responsive -->
            </div>
          </div>
          <!-- /.row -->
        </div>
        <!-- ./box-body -->
        <!-- /.box-footer -->
      </div>
      <!-- /.box -->
    </div>
    <!-- /.col -->
  </div>
  <!-- /.row -->

</section>
<!-- /.content -->
{{#extend 'scripts'}}
  <!-- Sparkline -->
  <script src="/admin/plugins/sparkline/jquery.sparkline.min.js"></script>
  <!-- ChartJS 1.0.1 -->
  <script src="/admin/plugins/chartjs/Chart.min.js"></script>
  <!-- GG maps API -->
